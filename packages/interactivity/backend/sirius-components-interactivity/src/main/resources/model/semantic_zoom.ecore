    <eStructuralFeatures xsi:type="ecore:EReference" name="style" lowerBound="1" eType="ecore:EClass ../../../../../sirius-components-view-diagram/src/main/resources/model/diagram.ecore#//NodeStyleDescription"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="filters" upperBound="-1"
        eType="#//GraphicalFilter" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ByDepth" eSuperTypes="#//GraphicalFilter"/>
  <eClassifiers xsi:type="ecore:EClass" name="ByEdgeNumber" eSuperTypes="#//GraphicalFilter"/>
</ecore:EPackage>
