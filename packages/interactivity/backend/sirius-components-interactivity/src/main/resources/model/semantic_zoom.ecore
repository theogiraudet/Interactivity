<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="semantic_zoom" nsURI="http://www.eclipse.org/sirius-web/semantic_zoom"
    nsPrefix="semantic_zoom">
  <eClassifiers xsi:type="ecore:EClass" name="SemanticZoom" eSuperTypes="interactivity.ecore#//InteractiveFeature"/>
  <eClassifiers xsi:type="ecore:EClass" name="LevelOfDetail" eSuperTypes="interactivity.ecore#//Action">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="GraphicalFilter" abstract="true" interface="true"/>
  <eClassifiers xsi:type="ecore:EClass" name="NodeMappingStyleOverride" eSuperTypes="interactivity.ecore#//NodeMappingReference">
    <eStructuralFeatures xsi:type="ecore:EReference" name="nodeDescription" lowerBound="1"
