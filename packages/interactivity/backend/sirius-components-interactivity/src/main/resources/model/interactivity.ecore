<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="interactivity" nsURI="http://www.eclipse.org/sirius-web/interactivity"
    nsPrefix="interactivity">
  <eClassifiers xsi:type="ecore:EClass" name="Interactivity">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="domainId" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="diagramEditors" upperBound="-1"
        eType="#//DiagramEditor" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Interaction" abstract="true" interface="true"/>
  <eClassifiers xsi:type="ecore:EClass" name="Action" abstract="true" interface="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="InteractiveFeature" abstract="true"
      interface="true"/>
  <eClassifiers xsi:type="ecore:EClass" name="GraphicZoom" eSuperTypes="#//Interaction">
    <eStructuralFeatures xsi:type="ecore:EReference" name="levels" upperBound="-1"
        eType="#//ZoomLevel" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ZoomLevel" eSuperTypes="#//Command">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="level" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="actionsToTrigger" upperBound="-1"
        eType="#//Action"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Command" abstract="true" interface="true">
    <eStructuralFeatures xsi:type="ecore:EReference" name="actions" upperBound="-1"
        eType="#//Action"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DiagramEditor">
    <eStructuralFeatures xsi:type="ecore:EReference" name="interactiveFeatures" upperBound="-1"
        eType="#//InteractiveFeature" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="interactions" upperBound="-1"
        eType="#//Interaction" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="diagramDefinition" lowerBound="1"
        eType="ecore:EClass ../../../../../sirius-components-view-diagram/src/main/resources/model/diagram.ecore#//DiagramDescription"/>
  </eClassifiers>
</ecore:EPackage>
